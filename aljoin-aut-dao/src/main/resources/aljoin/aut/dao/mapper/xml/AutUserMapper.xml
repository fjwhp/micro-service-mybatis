<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="aljoin.aut.dao.mapper.AutUserMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="aljoin.aut.dao.entity.AutUser">
		<id column="id" property="id" />
		<result column="create_time" property="createTime" />
		<result column="last_update_time" property="lastUpdateTime" />
		<result column="version" property="version" />
		<result column="is_delete" property="isDelete" />
<!-- 		<result column="last_update_user_id" property="lastUpdateUserId" />
		<result column="last_update_user_name" property="lastUpdateUserName" />
		<result column="create_user_id" property="createUserId" />
		<result column="create_user_name" property="createUserName" /> -->
		<result column="user_pwd" property="userPwd" />
		<result column="user_name" property="userName" />
		<result column="is_active" property="isActive" />
		<result column="is_account_expired" property="isAccountExpired" />
		<result column="is_account_locked" property="isAccountLocked" />
		<result column="is_credentials_expired" property="isCredentialsExpired" />
		<result column="full_name" property="fullName" />
		<result column="user_email" property="userEmail" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, create_time, last_update_time, version, is_delete, last_update_user_id, last_update_user_name, create_user_id, create_user_name, user_pwd, user_name, is_active, is_account_expired, is_account_locked, is_credentials_expired, full_name, user_email
    </sql>
    
    
    
    
    <select id="getMyUserList" resultType="AutUser">
    	select id,user_name from aut_user
    </select>

</mapper>
